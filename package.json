  "pluginsConfig": {
    "@nrwl/js": {
      "analyzeSourceFiles": true
    }
  },
  "optionalDependencies": {
    "@swc/core-darwin-x64": "^1.3.20",
    "@swc/core-linux-x64-gnu": "^1.3.20"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
