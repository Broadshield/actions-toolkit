{
  "name": "@broadshield/github-actions-octokit-hydrated",
  "version": "0.0.1",
  "description": "This adds a hydrated octokit with pagination, retries, and rate throttling",
  "main": "lib/octokit.js",
  "types": "lib/octokit.d.ts",
  "author": {
    "name": "Jamie Nelson",
    "email": "jamie@wearsafe.com"
  },
  "license": "MIT",
  "type": "commonjs",
  "keywords": ["GitHub", "Actions", "JavaScript", "TypeScript", "Inputs", "GitHub Actions"],
  "directories": {
    "src": "src",
    "lib": "lib",
    "test": "__tests__"
  },
  "files": ["src", "lib", "!.DS_Store"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Broadshield/actions-toolkit.git",
    "directory": "packages/github-actions-octokit-hydrated"
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.5.3"
  },
  "private": true,
  "dependencies": {
    "@broadshield/github-actions-core-typed-inputs": "workspace:^",
    "@octokit/core": "^4.1.0",
    "@octokit/graphql": "^5.0.4",
    "@octokit/plugin-paginate-rest": "^5.0.1",
    "@octokit/plugin-rest-endpoint-methods": "^6.7.0",
    "@octokit/plugin-throttling": "^4.3.2",
    "@octokit/request": "^6.2.2",
    "@octokit/rest": "^19.0.5",
    "@octokit/types": "^8.0.0",
    "@octokit/webhooks-types": "^6.7.0"
  },
  "devDependencies": {
    "@jackfranklin/test-data-bot": "^2.0.0",
    "@jest/globals": "^29.3.1",
    "@nrwl/eslint-plugin-nx": "^15.1.1",
    "@nrwl/jest": "^15.1.1",
    "@nrwl/linter": "latest",
    "@octokit/webhooks-schemas": "^6.7.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@typescript-eslint/typescript-estree": "^5.43.0",
    "babel-plugin-istanbul": "^6.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.14",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-actions": "^2.0.0",
    "eslint-plugin-editorconfig": "latest",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-json-schema-validator": "^4.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-sort-class-members": "^1.15.2",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^44.0.2",
    "github-action-readme-generator": "^1.2.7",
    "jest": "^29.3.1",
    "jest-circus": "^29.3.1",
    "jest-diff": "^29.3.1",
    "jest-junit": "^14.0.1",
    "jest-light-runner": "^0.4.1",
    "jest-snapshot": "^29.3.1",
    "jest-specific-snapshot": "^6.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^13.0.3",
    "nx": "^15.1.0",
    "prettier": "^2.7.1",
    "prettier-package-json": "^2.7.0",
    "prettier-plugin-organize-imports": "^3.2.0",
    "prettier-plugin-package": "^1.3.0",
    "prettier-plugin-properties": "^0.2.0",
    "prettier-plugin-sh": "^0.12.8",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4",
    "typescript-eslint-language-service": "^5.0.0",
    "yaml-eslint-parser": "^1.1.0"
  },
  "scripts": {
    "format": "prettier --write \"./{**/,}*.{ts,js,json,md,yml}\"",
    "lint": "eslint -c ./.eslintrc.json --color --cache --cache-location .eslintcache .",
    "prelint": "yarn run format && tsc --project tsconfig.json --noemit",
    "lint:tsc": "tsc --noEmit",
    "lint:fix": "yarn run lint --fix",
    "lint:markdown": "yarn run markdownlint \"**/*.md\"",
    "lint:markdown:fix": "yarn run lint:markdown --fix",
    "markdownlint": "markdownlint --config=.markdownlint.yaml --ignore-path=.markdownlintignore",
    "test:watch": "jest --watch",
    "test": "NODE_ENV=testing jest --runInBand --coverage",
    "test:ci": "NODE_ENV=testing jest --ci --reporters=default --reporters=jest-junit --runInBand",
    "typecheck": "tsc --noEmit",
    "start": "ts-node ./src/index.ts",
    "semantic-release": "semantic-release",
    "build:bundle": "rimraf dist;esbuild ./lib --bundle --sourcemap --platform=node --outfile=dist/index.cjs --target=node16 --format=cjs",
    "build": "tsc --build ./tsconfig.lib.json --verbose"
  },
  "release": {
    "branches": ["main"],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "build",
                "section": "Dependencies and Other Build Updates",
                "hidden": false
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": ["package.json"],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
